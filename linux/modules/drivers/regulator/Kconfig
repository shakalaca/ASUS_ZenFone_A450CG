config REGULATOR_PMIC_AVP
        tristate "PMIC Avondale Cove voltage regulator"
        help
          This driver controls a intel pmic avatele voltage output regulator

config MMC_SD_VCCSDIO_ALWAYS_ON
        bool "VCCSDIO always on"
        depends on REGULATOR_PMIC_AVP
        help
          This option controls whether VCCSDIO is AON or not
